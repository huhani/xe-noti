<query id="getNotiEndpoint" action="select">
    <tables>
        <table name="noti_endpoint" />
    </tables>
    <columns>
        <column name="*" />
    </columns>
    <conditions>
        <condition operation="equal" column="endpoint_srl" var="endpoint_srl" notnull="notnull" />
    </conditions>
</query>
