<query id="getNotiManualPushLog" action="select">
    <tables>
        <table name="noti_manual_push" />
    </tables>
    <columns>
        <column name="*" />
    </columns>
    <conditions>
        <condition operation="equal" column="manual_push_srl" var="manual_push_srl" filter="number" />
    </conditions>
</query>
