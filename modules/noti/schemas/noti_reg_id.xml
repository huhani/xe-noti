<table name="noti_reg_id">
	<column name="reg_srl" type="number" size="11" default="0" notnull="notnull" primary_key="primary_key" />
	<column name="member_srl" type="number" size="11" default="0" notnull="notnull" index="idx_member_srl" />
	<column name="noti_srl" type="number" size="11" default="0" notnull="notnull" index="idx_noti_srl" />
	<column name="ipaddress" type="varchar" size="128" notnull="notnull" />
	<column name="user_agent" type="varchar" size="250" notnull="notnull" />
	<column name="browser" type="varchar" size="10" notnull="notnull" brief="chrome, firefox, opera, etc.." />
	<column name="platform" type="varchar" size="50" notnull="notnull" default="unknown" brief="win, linux, macos" />

	<column name="dev_id" type="varchar" size="32" notnull="notnull" index="idx_dev_id" />
	<column name="reg_id" type="varchar" size="250" notnull="notnull" />

	<column name="click_count" type="number" size="11" default="0" notnull="notnull" index="idx_click_count" />
	<column name="is_clicked" type="char" size="1" default="N" notnull="notnull" index="idx_is_clicked" />
	<column name="is_recv" type="char" size="1" default="N" notnull="notnull" index="idx_is_recv" />

	<column name="recv_date" type="date" index="idx_recv_date" />
	<column name="last_click" type="date" index="idx_last_click" />
	<column name="last_post" notnull="notnull" type="date" index="idx_last_post" />
	<column name="regdate" notnull="notnull" type="date" index="idx_regdate" />
</table>
